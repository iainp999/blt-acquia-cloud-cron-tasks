{
  "name": "vbouchet31/blt-acquia-cloud-cron-tasks",
    "type": "library",
    "description": "Acquia Cloud Cron Tasks manager for Acquia BLT",
    "keywords": ["acquia", "blt", "cron"],
    "homepage": "https://github.com/vbouchet31/blt-acquia-cloud-cron-tasks",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Bouchet",
            "role": "Maintainer"
        }
    ],
  "require": {
    "typhonius/acquia-php-sdk-v2": "^2.5.0",
    "cweagans/composer-patches": "^1.7"
  },
  "require-dev": {
    "acquia/blt": "^13"
  },
  "autoload": {
    "psr-4": {
      "AcquiaCloudCronTasks\\": "./src/"
    }
  },
  "extra": {
    "blt": {
      "update": true
    },
    "enable-patching": true,
    "patches": {
      "typhonius/acquia-php-sdk-v2": {
        "Use PUT method for Crons Update": "https://patch-diff.githubusercontent.com/raw/typhonius/acquia-php-sdk-v2/pull/290.patch"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "acquia/blt": true
    }
  }
}
